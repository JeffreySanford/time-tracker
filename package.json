{
  "name": "@time-tracker/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:frontend": "nx run time-forge:serve",
    "start:frontend:android": "nx run time-forge:serve --host 0.0.0.0",
    "start:backend": "nx run api:serve",
    "start:backend:dev": "cross-env NODE_ENV=development nx run api:serve",
    "start:all": "concurrently --prefix-colors \"cyan,magenta\" --prefix \"[{name}]\" --names \"frontend,backend\" \"npm run start:frontend\" \"npm run start:backend\"",
    "start:all:dev": "cross-env NODE_ENV=development concurrently --prefix-colors \"cyan,magenta\" --prefix \"[{name}]\" --names \"frontend,backend\" \"npm run start:frontend\" \"npm run start:backend:dev\"",
    "start:all:android:dev": "concurrently --prefix-colors \"cyan,magenta,green\" --prefix \"[{name}]\" --names \"frontend,backend,android\" \"npm run start:frontend:android\" \"npm run start:backend:dev\" \"node scripts/android-dev.js\"",
    "build:android": "nx run time-forge:build && npx cap sync android",
    "build:frontend": "nx run time-forge:build",
    "build:backend": "nx run api:build",
    "android:dev": "node scripts/android-dev.js",
    "android:build": "nx run time-forge:build && npx cap sync android && npx cap open android",
    "android:sync": "npx cap sync android",
    "android:open": "npx cap open android",
    "cap:sync": "npx cap sync",
    "cap:copy": "npx cap copy",
    "lint:md": "git ls-files -z -- \"*.md\" | xargs -0 -n 50 npx markdownlint-cli2 -c .markdownlint.json --ignore 'node_modules/**'",
    "lint:md:fix": "git ls-files -z -- \"*.md\" | xargs -0 -n 50 npx markdownlint-cli2 --fix -c .markdownlint.json --ignore 'node_modules/**'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.6",
    "@angular/cdk": "^20.1.5",
    "@angular/common": "~20.1.0",
    "@angular/compiler": "~20.1.0",
    "@angular/core": "~20.1.0",
    "@angular/forms": "~20.1.0",
    "@angular/material": "^20.1.5",
    "@angular/platform-browser": "~20.1.0",
    "@angular/platform-browser-dynamic": "^20.1.6",
    "@angular/router": "~20.1.0",
    "@capacitor/android": "^7.4.2",
    "@capacitor/core": "^7.4.2",
    "@capacitor/ios": "^7.4.2",
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/platform-express": "^11.1.6",
    "@ngrx/effects": "^20.0.0",
    "@ngrx/entity": "^20.0.0",
    "@ngrx/store": "^20.0.0",
    "@ngrx/store-devtools": "^20.0.0",
    "@nx/node": "21.3.11",
    "mongoose": "^8.17.1",
    "rxjs": "~7.8.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "~1.19.1",
    "@analogjs/vitest-angular": "~1.19.1",
    "@angular-devkit/core": "~20.1.0",
    "@angular-devkit/schematics": "~20.1.0",
    "@angular/build": "~20.1.0",
    "@angular/cli": "~20.1.0",
    "@angular/compiler-cli": "~20.1.0",
    "@angular/language-service": "~20.1.0",
    "@capacitor/cli": "^7.4.2",
    "@eslint/js": "^9.8.0",
    "@nx/angular": "21.3.11",
    "@nx/devkit": "21.3.11",
    "@nx/eslint": "21.3.11",
    "@nx/eslint-plugin": "21.3.11",
    "@nx/js": "21.3.11",
    "@nx/playwright": "21.3.11",
    "@nx/vite": "21.3.11",
    "@nx/web": "21.3.11",
    "@nx/workspace": "21.3.11",
    "@playwright/test": "^1.36.0",
    "@schematics/angular": "~20.1.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/node": "18.16.9",
    "@typescript-eslint/utils": "^8.29.0",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.0",
    "angular-eslint": "^20.0.0",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-playwright": "^1.6.2",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "mongodb-memory-server": "^10.2.0",
    "nx": "21.3.11",
    "prettier": "^2.6.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.3.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.0.0",
    "vitest": "^3.0.0"
  }
}
